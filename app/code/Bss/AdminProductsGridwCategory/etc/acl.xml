                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog" title="Catalog" translate="title" sortOrder="30">
                    <resource id="Bss_AdminProductsGridwCategory::action_product" title="Bss module action"
                              translate="title" sortOrder="50"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
