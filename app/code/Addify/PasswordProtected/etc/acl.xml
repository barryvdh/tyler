<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
   <acl>
       <resources>
           <resource id="Magento_Backend::admin">
               <resource id="Addify_Extensions::addifyextensions" title="Addify Extensions" sortOrder="51">
                   <resource id="Addify_PasswordProtected::passwordprotectedparent" title="Addify Password Protected" sortOrder="10" translate="title">
                       <resource id="Addify_PasswordProtected::managepasswordprotected" title="Manage password Protected" sortOrder="10" />
                       <resource id="Addify_PasswordProtected::passwordprotected_configuration" title="Configuration" sortOrder="20" translate="title" />
                   </resource>
               </resource>
               <resource id="Magento_Backend::stores">
                   <resource id="Magento_Backend::stores_settings">
                       <resource id="Magento_Config::config">
                           <resource id="Addify_PasswordProtected::passwordprotected_config" title="Addify Password Protected configuration" sortOrder="100" />
                       </resource>
                   </resource>
               </resource>
           </resource>
       </resources>
   </acl>
</config>